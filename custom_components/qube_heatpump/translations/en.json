  "services": {
    "reconfigure": {
      "name": "Trigger reconfigure flow",
      "description": "Open the integration's reconfigure flow so you can update the Modbus host or port from the Home Assistant UI.",
      "fields": {
        "entry_id": {
          "name": "Config entry ID",
          "description": "Optional integration entry_id to reconfigure. If omitted, the service will attempt to resolve the single configured entry automatically."
        }
      }
    },
    "write_register": {
      "name": "Write holding register",
      "description": "Write a value to a Modbus holding register exposed by the Qube heat pump. Supports uint16, int16, and float32 registers and mirrors the behaviour of Home Assistant's native modbus.write_register service.",
      "fields": {
        "address": {
          "name": "Register address",
          "description": "The Modbus holding register address to write (0-based addressing)."
        },
        "value": {
          "name": "Value",
          "description": "The value to store in the register. Float values are encoded as big-endian IEEE 754 when data_type is float32."
        },
        "data_type": {
          "name": "Data type",
          "description": "Register data type. Use float32 for 32-bit floating point registers."
        },
        "entry_id": {
          "name": "Config entry ID",
          "description": "Optional entry_id of the target integration instance. Needed when multiple Qube heat pumps are configured."
        },
        "label": {
          "name": "Hub label",
          "description": "Optional hub label to target a specific device when multiple hubs exist. The label is derived from the integration entry title (e.g. 'Qube Heat Pump (qube.local)' → 'qube_local'). Ignored if entry_id is provided."
        }
      }
    }
  },
  "issues": {
    "connection_failed": {
      "title": "Connection to Qube heat pump failed",
      "fix_flow": {
        "step": {
          "confirm": {
            "title": "Connection to Qube heat pump failed",
            "description": "The integration has been unable to connect to the Qube heat pump at **{host}** after multiple attempts.\n\nPossible causes:\n- The heat pump is powered off or in standby\n- Network connectivity issues\n- IP address has changed\n\nWould you like to reconfigure the connection settings?"
          }
        }
      }
    }
  },
  "exceptions": {
    "connection_failed": {
