    },
    "write_register_failed": {
      "message": "Failed to write register at address {address}."
    },
    "write_switch_failed": {
      "message": "Failed to write switch {entity_id}."
    },
    "write_setpoint_failed": {
      "message": "Failed to write setpoint {entity_id}."
    },
    "not_connected": {
